program ProjetoLivraria;

uses
  Vcl.Forms,
  Inicio in 'Inicio.pas' {Form1},
  Usuarios in 'Usuarios.pas' {Form2},
  Editora in 'Editora.pas' {Form3},
  FormaPagamento in 'FormaPagamento.pas' {Form4},
  Livro in 'Livro.pas' {frmCadLivro},
  ClienteFornecedor in 'ClienteFornecedor.pas' {Form6};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm4, Form4);
  Application.CreateForm(TfrmCadLivro, frmCadLivro);
  Application.CreateForm(TForm6, Form6);
  Application.Run;
end.
